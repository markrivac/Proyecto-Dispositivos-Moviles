    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.davidcabello.proyectomoviles.Inicio"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.davidcabello.proyectomoviles.Login" android:theme="@style/AppTheme.Dark"/>
        <activity android:name="com.example.davidcabello.proyectomoviles.Registro" android:theme="@style/AppTheme.Dark"/>
        <activity android:name="com.example.davidcabello.proyectomoviles.Usuario" android:theme="@style/AppTheme.Dark"/>
        <activity android:name="com.example.davidcabello.proyectomoviles.Categorias" android:theme="@style/AppTheme.Dark" />
        <activity android:name="com.example.davidcabello.proyectomoviles.InicioUser" android:theme="@style/AppTheme.Dark"/>
        <activity android:name="com.example.davidcabello.proyectomoviles.Categoria" android:theme="@style/AppTheme.Dark"/>
        <activity android:name="com.example.davidcabello.proyectomoviles.Pregunta" android:theme="@style/AppTheme.Dark"/>

    </application>

</manifest>


